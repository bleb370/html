class Parrot:
    species = "birds"
    def _init_(self, name, age):
     self.name = name
     self.age = age
blu = Parrot("Blu", 10)
woo = Parrot("Woo", 15)
print("Blu is a {}". format(blu.species))
print("{} is {} years old".format(blue.name, blue .age))
print("{} is {} years old".format(woo.name, woo.age))